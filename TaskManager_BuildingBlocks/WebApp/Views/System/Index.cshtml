@{
    ViewData["Title"] = "Servers";
}
<div class="container">
    <h1>@ViewData["Title"]</h1>

    <div class="list-group">
        @foreach (var sys in ViewData["Systems"] as List<BusinessLayer.Server>)
        {
            <a href="#" class="list-group-item">
                <p>@sys.SystemName</p>
            </a>
        }
    </div>
</div>